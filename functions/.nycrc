{
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "all": true,
    "include": [
      "src/**/*.[tj]s?(x)"
    ],
    "exclude": [
      "src/**/*.spec.[tj]s?(x)"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage",
    "branches": 0,
    "lines": 0,
    "functions": 0,
    "statements": 0
}