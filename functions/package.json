{
  "name": "functions",
  "scripts": {
    "lint": "eslint --cache \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --cache --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "build": "tsc",
    "build-local": "npm run env-local && tsc",
    "dev": "npm run lint && tsc -w",
    "api-dev": "nodemon src/server.ts",
    "deploy": "npm run env && firebase deploy --only functions",
    "logs": "firebase functions:log",
    "start": "node -r ./lib/tracing lib/server",
    "env": "test -f env.json && firebase functions:config:unset env && firebase functions:config:set env=\"$(cat env.json)\" || echo \"Please add the file env.json before deploy.\"",
    "env-local": "firebase functions:config:get > .runtimeconfig.json",
    "test": "cross-env TS_NODE_FILES=true mocha --exit --require ts-node/register --colors \"{,!(node_modules)/**}/*.spec.ts\" --timeout 5000",
    "test:coverage": "nyc npm run test",
    "test:api": "ts-mocha -p tsconfig.json \"**/*.spec.api.ts\"",
    "test:db": "ts-mocha -p tsconfig.json \"**/*.spec.db.ts\"",
    "test:e2e": "ts-mocha -p tsconfig.json \"{,!(node_modules)/**}/*.spec.ts\" \"**/*.spec.api.ts\" \"**/*.spec.db.ts\" --timeout 5000"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@firebase/app": "^0.7.30",
    "@firebase/app-compat": "^0.1.31",
    "@firebase/firestore": "^3.4.14",
    "@google-cloud/firestore": "^5.0.2",
    "@google-cloud/logging-bunyan": "^3.2.0",
    "@grpc/grpc-js": "^1.5.3",
    "@opentelemetry/api": "^1.0.4",
    "@opentelemetry/auto-instrumentations-node": "^0.27.2",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.27.0",
    "@opentelemetry/sdk-node": "^0.27.0",
    "@types/compression": "^1.7.2",
    "axios": "^0.24.0",
    "body-parser": "^1.19.1",
    "bunyan": "^1.8.15",
    "cheerio": "^1.0.0-rc.10",
    "compression": "^1.7.4",
    "domino": "^2.1.6",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "express-joi-validation": "^5.0.1",
    "express-jsdoc-swagger": "^1.6.7",
    "faker": "^5.5.3",
    "firebase-admin": "^11.0.1",
    "firebase-functions": "^3.22.0",
    "fishery": "^2.1.0",
    "fs-extra": "^10.0.0",
    "html-metadata-parser": "^2.0.4",
    "htmlparser": "^1.7.7",
    "ioredis": "^4.28.5",
    "joi": "^17.6.0",
    "joi-extract-type": "~15.0.8",
    "libhoney": "^3.0.0",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.1",
    "multer": "^1.4.4",
    "node-html-parser": "^5.2.0",
    "node-ts-cache": "^4.4.0",
    "node-ts-cache-storage-ioredis": "^4.4.0",
    "node-ts-cache-storage-memory": "^4.4.0",
    "page-metadata-parser": "^1.1.4",
    "set-script": "^1.0.1",
    "sharp": "^0.30.5",
    "typesafe-functions": "^1.0.0",
    "typesaurus": "^7.2.0",
    "unfurl.js": "^5.6.3",
    "url-parse": "^1.5.10",
    "uuidv4": "^6.2.12"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.3",
    "@types/cheerio": "^0.22.31",
    "@types/faker": "^5.5.9",
    "@types/fs-extra": "^9.0.13",
    "@types/hapi__joi": "^17.1.8",
    "@types/ioredis": "^4.28.8",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^9.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.5",
    "@types/sharp": "^0.30.5",
    "@types/url-parse": "^1.4.8",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.8.0",
    "chai": "^4.3.6",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "firebase-functions-test": "^2.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npm run lint:fix"
  },
  "private": true
}
